{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "chrome": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/forbid-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/label-has-for": 0,
    "consistent-return": 0,
    "comma-dangle": 0,
    "spaced-comment": 0,
    "global-require": 0,
    "quotes": ["error", "double"],
    "arrow-parens": 0,
    "no-eval": 0,
    "no-param-reassign": 0,
    "no-mixed-operators": [
      "error",
      {
        "groups": [],
        "allowSamePrecedence": true
      }
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "react/jsx-indent": [2, 2],
    "react/jsx-indent-props": [2, 2]
  },
  "plugins": ["react"]
}
